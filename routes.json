[
    {
        "name": "Get user info",
        "url": "/cached/user/(\\d+)",
        "method": "GET",
        "type": "data",
        "internal_url": "/user",
        "variables": [
            {
                "name": "user_id",
                "description": "The user id to query",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "responses": [
            {
                "status": 200,
                "description": "User fetched",
                "override": true,
                "schema": "User"
            },
            {
                "status": 404,
                "description": "User not found."
            }
        ],
        "require_auth": false
    },
    {
        "name": "Get video info",
        "url": "/cached/video-info/(\\d+)",
        "method": "GET",
        "type": "data",
        "internal_url": "/video",
        "variables": [
            {
                "name": "video_id",
                "description": "The video id to query",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "responses": [
            {
                "status": 200,
                "description": "Video metadata fetched!",
                "override": true,
                "schema": "Video"
            },
            {
                "status": 403,
                "description": "Video was marked as private and you don't have permission to access it"
            },
            {
                "status": 404,
                "description": "Video not found."
            }
        ],
        "require_auth": false
    },
    {
        "name": "Get video",
        "url": "/cached/video/(\\d+)",
        "method": "GET",
        "type": "cdn",
        "internal_url": "/videos",
        "variables": [
            {
                "name": "video_id",
                "description": "The video id to fetch from",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "responses": [
            {
                "status": 403,
                "description": "Video was marked as private and you don't have permission to access it"
            }
        ],
        "require_auth": false,
        "pass_roles": true
    },
    {
        "name": "Get user avatar",
        "url": "/cached/avatar/(\\d+)",
        "method": "GET",
        "type": "avatargen",
        "internal_url": "/",
        "variables": [
            {
                "name": "user_id",
                "description": "The user id to generate avatar for",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "require_auth": false
    },








    {
        "name": "Get recommended videos",
        "url": "/live/recommended/",
        "method": "GET",
        "type": "recommended",
        "internal_url": "/user",
        "variables": [],
        "require_auth": true
    },
    {
        "name": "Login",
        "url": "/live/login",
        "method": "POST",
        "type": "users",
        "internal_url": "/login",
        "variables": [
            {
                "name": "username",
                "description": "Username to log into",
                "source": "headers",
                "query": "username",
                "type": "str",
                "max": 255
            },
            {
                "name": "password",
                "description": "Password to log in with",
                "source": "headers",
                "query": "password",
                "type": "str"
            }
        ],
        "responses": [
            {
                "status": 400,
                "description": "Username or password not provided"
            },
            {
                "status": 403,
                "description": "Invalid username or password"
            }
        ],
        "require_auth": false
    },
    {
        "name": "Register",
        "url": "/live/register",
        "method": "POST",
        "type": "users",
        "internal_url": "/register",
        "variables": [
            {
                "name": "username",
                "description": "Your public username",
                "source": "headers",
                "query": "username",
                "type": "str",
                "max": 255
            },
            {
                "name": "password",
                "description": "Password to register with",
                "source": "headers",
                "query": "password",
                "type": "str"
            }
        ],
        "responses": [
            {
                "status": 400,
                "description": "Username or password not provided / Username already taken"
            }
        ],
        "require_auth": false
    },
    {
        "name": "Upload video",
        "url": "/live/upload-video",
        "method": "POST",
        "type": "videos",
        "internal_url": "/upload",
        "variables": [
            {
                "name": "description",
                "description": "Description to add to the video (255 characters max)",
                "source": "form",
                "type": "str"
            },
            {
                "name": "series",
                "description": "A random string for the series. To add to the series put the same string in another upload. Cannot be someone elses series.",
                "source": "form",
                "type": "str"
            }
        ],
        "responses": [
            {
                "status": 200,
                "description": "Video uploaded!",
                "override": true,
                "schema": "Video"
            },
            {
                "status": 400,
                "description": "Description was too long. / Unsupported file format (mp4 only)"
            }
        ],
        "require_auth": true,
        "attach_content": true
    },
    {
        "name": "Delete video",
        "url": "/live/delete-video/(\\d+)",
        "method": "DELETE",
        "type": "videos",
        "internal_url": "/delete",
        "variables": [
            {
                "name": "video-id",
                "description": "The id of the video to delete. Has to be your own",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "responses": [
            {
                "status": 403,
                "description": "You did not create this video."
            },
            {
                "status": 404,
                "description": "Video not found."
            }
        ],
        "require_auth": true
    },
    {
        "name": "Mark video as watched",
        "url": "/live/set-watched/(\\d+)",
        "method": "POST",
        "type": "interactions",
        "internal_url": "/watched",
        "variables": [
            {
                "name": "video-id",
                "description": "The id of the video to mark as watched.",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "responses": [
            {
                "status": 404,
                "description": "Video not found."
            },
            {
                "status": 412,
                "description": "You have already marked the video as watched."
            }
        ],
        "require_auth": true
    },
    {
        "name": "Like video",
        "url": "/live/like-video/(\\d+)",
        "method": "POST",
        "type": "interactions",
        "internal_url": "/like",
        "variables": [
            {
                "name": "video-id",
                "description": "The id of the video to like.",
                "source": "url",
                "query": 0,
                "type": "int"
            }
        ],
        "responses": [
            {
                "status": 404,
                "description": "Video not found."
            },
            {
                "status": 412,
                "description": "You have already liked this video"
            }
        ],
        "require_auth": true
    }
]
